{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["unused-imports"],
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "warn",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^\\$",
        "args": "after-used",
        "argsIgnorePattern": "^\\$"
      }
    ],
    "react/jsx-curly-brace-presence": "warn",
    "react/display-name": "off"
  }
}
